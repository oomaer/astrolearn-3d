    "cannon-es": "^0.20.0",
    "pinia": "^2.1.4",
    "sass": "^1.65.1",
    "three": "^0.144.0",
    "vue": "^3.3.4",
    "vue-accessible-color-picker": "^4.1.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.2",
